{
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
    "extends": "airbnb",
    "parser": "babel-eslint",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "react/jsx-filename-extension":  0,
        "no-use-before-define": 0,
        "jsx-a11y/href-no-hash": "off"
    }
}
